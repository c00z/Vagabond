<h1>Login</h1>

<%= form_for @user, url: "/sessions", method: "post" do |f| %>
  <div>
    <%= f.email_field :email, placeholder: "Email" %>
  </div>
  <div>
    <%= f.password_field :password, placeholder: "Password" %>
  </div>
  <a class="waves-effect waves-light btn"><%= f.submit "Log In" %></a>
<% end %>
